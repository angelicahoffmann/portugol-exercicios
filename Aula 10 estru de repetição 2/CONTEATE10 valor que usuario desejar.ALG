Algoritmo "conteAte10"

Var

       valor, contador: Inteiro

Inicio
     contador <- 0
     escreva("Quer contar ate quanto? ")
     Leia(valor)
     Enquanto (contador >= valor) faca
          EscrevaL(contador)
          contador <- contador + 1
    FimEnquanto
       EscrevaL("Terminei de contar ")
Fimalgoritmo